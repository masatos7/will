<!DOCTYPE html>
<html>
<head>
  <title>Will</title>
</head>
<body>
  <% if session[:admin_email] %>
    <div class="identity">Admin Logged in as: <%= session[:admin_email] %></div>
    <%= link_to("AdminLogout", :controller =>'admin_access', :action => 'logout') %>
  <% end %>
  
  <% if session[:email] %>
    <div class="identity">Logged in as: <%= session[:email] %></div>
    <%= link_to("Logout", :controller => 'access', :action => 'logout') %>
  <% end %>



  <% if !flash[:notice].blank? %>
    <div class="notice">
      <%= flash[:notice] %>
    </div>
  <% end %>
  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>
  <%= yield %>
  <%= debug(params) if Rails.env.development? %>
</body>
</html>
