<div class="container">
  <h2>ユーザー登録</h2>
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render(:partial => "form", :locals => {:f => f}) %>
    <div class="text-center">
      <%= submit_tag("登録する", :class => 'btn btn-success btn-lg') %>
    </div>
  <% end %>
<%= render "devise/shared/links" %>
</div><!-- container -->